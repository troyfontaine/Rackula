{
  "name": "rackula",
  "version": "0.7.0",
  "description": "Drag and drop rack visualizer",
  "author": "ggfevans",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RackulaLives/Rackula.git"
  },
  "homepage": "https://github.com/RackulaLives/Rackula",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--max-old-space-size=8192 vitest",
    "test:run": "NODE_OPTIONS=--max-old-space-size=8192 vitest run",
    "test:e2e": "playwright test",
    "test:e2e:dev": "playwright test --config playwright.dev.config.ts",
    "test:e2e:smoke": "playwright test --config playwright.smoke.config.ts",
    "test:coverage": "NODE_OPTIONS=--max-old-space-size=8192 vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "husky",
    "process-images": "npx tsx scripts/process-images.ts",
    "generate-bundled-images": "npx tsx scripts/generate-bundled-images.ts",
    "update-contributors": "npx tsx scripts/update-contributors.ts",
    "refresh-lockfile": "rm -rf node_modules package-lock.json && npm install",
    "deps:status": "npm outdated || true"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.6.1",
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.9",
    "@types/pako": "^2.0.4",
    "@types/umami-browser": "^2.18.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "eslint-plugin-testing-library": "^7.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^17.0.0",
    "happy-dom": "^20.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.0",
    "sharp": "^0.34.5",
    "svelte": "^5.46.4",
    "svelte-check": "^4.3.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.17"
  },
  "lint-staged": {
    "*.{js,ts,svelte,json,md,css,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@iconify/svelte": "^5.2.1",
    "@lucide/svelte": "^0.562.0",
    "bits-ui": "^2.15.4",
    "debug": "^4.4.3",
    "fuse.js": "^7.1.0",
    "js-yaml": "^4.1.1",
    "jspdf": "^4.0.0",
    "jszip": "^3.10.1",
    "marked": "^17.0.1",
    "nanoid": "^5.1.6",
    "pako": "^2.1.0",
    "paneforge": "^1.0.2",
    "panzoom": "^9.4.3",
    "qrcode": "^1.5.4",
    "simple-icons": "^16.2.0",
    "vaul-svelte": "^0.3.2",
    "zod": "^4.1.13"
  }
}
